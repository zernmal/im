<div id="header" >
	<div class="headerwrap" class="clearfix">
		<div class="logo clearfix">
			<img src="/public/skin/img/site_logo.jpg" alt="">
		</div>
		<div class="search">
			<form action="/search" class="clearfix">
				<input type="text" class="text" name="keyword" >
				<input type="submit" value="O" class="subbtn">
			</form>
		</div>
		<div class="nav">
			<ul class="level1">
				<% for( var i in nav) { %>	
					<li><a href="<%= nav[i].url %>"><%= nav[i].name %></a></li>
				<% } %>
			</ul>
		</div>
	</div>
</div>