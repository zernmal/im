    <!-- Bootstrap core CSS -->
	<link rel='stylesheet' href='/public/plugin/bootstrap/css/bootstrap.min.css' />
	<link rel='stylesheet' href='/public/css/admin.css' />
    <!--[if lt IE 9]>
      <script src="/public/plugin/html5shiv/html5shiv.min.js"></script>
